{
    "name": "fakerphp/ghanaian",
    "type": "library",
    "description": "Ghanaian specific extensions for Faker",
    "keywords": [
        "faker",
        "fixtures",
        "data"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Prince Dorcis",
            "email": "princedorcis@gmail.com"
        }
    ],
    "require": {
        "php": "^7.3 || ^8.0",
        "fakerphp/faker": "dev-main"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "Faker\\Ghanaian\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Faker\\Test\\Ghanaian\\": "test/"
        }
    },
    "config": {
        "sort-packages": true
    }
}
